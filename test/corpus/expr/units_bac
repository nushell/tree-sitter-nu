(nu_script
  (pipeline
    (pipe_element
      (command
        (cmd_identifier)
        (val_duration
          (val_number)
          (duration_unit)))))
  (pipeline
    (pipe_element
      (command
        (cmd_identifier)
        (val_number)
        (val_string))))
  (pipeline
    (pipe_element
      (command
        (cmd_identifier)
        (val_filesize
          (val_number)
          (filesize_unit)))))
  (pipeline
    (pipe_element
      (command
        (cmd_identifier)
        (val_filesize))))
